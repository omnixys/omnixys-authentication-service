    "node": ">=24.10.0",
    "pnpm": ">=10.19.0"
  },
  "scripts": {
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo",
    "build": "pnpm run clean && nest build",
    "format": "prettier --cache --write \"src/**/*.{ts,js,json,prisma,md,yaml}\" \"__tests__/**/*.ts\"",
    "format:check": "prettier --cache --check \"src/**/*.{ts,js,json,prisma,md,yaml,yml}\" \"__test__/**/*.ts\"",
    "start": "pnpm run clean && nest start",
    "start:dev": "pnpm run clean && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e:debug": "NODE_OPTIONS='--experimental-vm-modules' jest --runInBand --detectOpenHandles --config ./__tests__/jest-e2e.json",
    "test:e2e2": "jest --config ./__tests__/jest-e2e.json",
    "test:e2e": "NODE_OPTIONS='--experimental-vm-modules' jest --config ./__tests__/e2e/jest-e2e.json --passWithNoTests",
    "typedoc": "typedoc",
    "depcheck": "depcheck",
    "test:e2e:auth": "NODE_OPTIONS='--experimental-vm-modules' jest --config ./jest-e2e.json --maxWorkers=4 --coverage",
    "test:e2e:auth:login": "pnpm run test:e2e:auth -- __tests__/e2e/auth/auth.login.e2e-spec.ts",
        "test:e2e:auth:signup": "pnpm run test:e2e:auth -- __tests__/e2e/auth/auth.signup.e2e-spec.ts",
    "test:e2e:auth:user": "pnpm run test:e2e:auth -- __tests__/e2e/auth/auth.user.e2e-spec.ts",
    "test:e2e:auth:admin": "pnpm run test:e2e:auth -- __tests__/e2e/auth/auth.admin.e2e-spec.ts"
  },
